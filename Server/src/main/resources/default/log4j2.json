{
  "Configuration": {
    "Appenders": {
      "appender": [{
        "type": "Console",
        "name": "STDOUT",
        "target": "SYSTEM_OUT",
        "PatternLayout": { "pattern": "[%d{dd/MMM/yyyy|HH:mm:ss}] %-5p - %C:%L - %m%n" }
        
      }]
    },
    "Loggers": {
      "logger": [{
        "name": "org.gusdb",
        "level": "info"
      },{
        "name": "org.eupathdb",
        "level": "info"
      },{
        "name": "org.apidb",
        "level": "info"
      },{
        "name": "org.orthomcl",
        "level": "info"
      }],
      "root" : {
        "level": "warn",
        "AppenderRef": { "ref": "STDOUT" }
      }
    }
  }
}
